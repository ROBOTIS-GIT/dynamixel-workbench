<!-- -*- mode: XML -*- -->

<launch>
    <node name="dynamixel_workbench_multi_port_controllers" pkg="dynamixel_workbench_controllers" type="dynamixel_workbench_multi_port_control" required="true" output="screen">
        <!-- <rosparam file="$(find dynamixel_workbench_monitor)/param/dynamixel.yaml" command="load"/> -->
        <rosparam>
          pan_motor_:
            device_name_: "/dev/ttyUSB1"
            baud_rate_: 1000000
            motor_model_: "XM430_W210_R"
            protocol_version_: 2.0
            motor_id_: 1
          tilt_motor_:
            device_name_: "/dev/ttyUSB0"
            baud_rate_: 1000000
            motor_model_: "RX_28"
            protocol_version_: 1.0
            motor_id_: 2
        </rosparam>
    </node>
</launch>
